{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "index",
  "compilerOptions": {
    "tsConfigPath": "tsconfig.build.json",
    "deleteOutDir": true,
    "builder": {
      "type": "swc",
      "options": {
        "swcrcPath": "../../.swcrc"
      }
    },
    "typeCheck": true
  }
}
